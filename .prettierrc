{
  "importOrder": [
    "@ant-design/icons",
    "@components",
    "@atoms",
    "@molecules",
    "@organisms",
    "@redux",
    "@utils",
    "@models",
    "@src",
    "@hooks",
    "@constants",
    "@styles",
    "@assets",
    "./"
